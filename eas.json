{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": {
        "buildType": "apk",
        "image": "latest"
      },
      "env": {
        "CS_API": "https://api.mmb.io.vn/cs",
        "BACKUP_CS_API": "https://mmb-be-dotnet.onrender.com",
        "PY_API": "https://api.mmb.io.vn/py",
        "BACKUP_PY_API": "https://sep490-manga-mystery-box-pybe.onrender.com"
      }
    },
    "production": {
      "autoIncrement": true,
      "channel": "production",
      "android": {
        "image": "latest"
      }
    }
  },
  "submit": {
    "production": {}
  }
}